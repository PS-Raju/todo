CREATE TABLE user (id MEDIUMINT NOT NULL AUTO_INCREMENT, firstName CHAR(30) NOT NULL, lastName CHAR(30),mobile CHAR(30) NOT NULL ,PRIMARY KEY (id));

CREATE TABLE list(id MEDIUMINT NOT NULL AUTO_INCREMENT, name CHAR(30) NOT NULL, userId MEDIUMINT NOT NULL, category CHAR(30),PRIMARY KEY (id));

CREATE TABLE item(id MEDIUMINT NOT NULL AUTO_INCREMENT, title CHAR(255) NOT NULL, listId MEDIUMINT ,userId MEDIUMINT NOT NULL, isCompleted BOOLEAN NOT NULL, dueDate TIMESTAMP NULL DEFAULT NULL, creationDate TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL, PRIMARY KEY (id));